{
  "schemaVersion": 1,
  "id": "ct-chatbox",
  "version": "${version}",
  "name": "Chatbox Server",
  "description": "Chatbox WebSocket server for the project CT Minecraft server",
  "authors": [
    "AlexDevs"
  ],
  "contact": {
    "sources": "https://github.com/project-connecticut/ct-chatbox"
  },
  "license": "MIT License",
  "icon": "assets/ct-chatbox/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "ct.chatbox.Chatbox"
    ]
  },
  "mixins": [
    "ct-chatbox.mixins.json",
    {
      "config": "ct-chatbox.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "ct-server": ">=1.3.1"
  }
}
