  "description": "Chatbox WebSocket server for the ReconnectedCC Minecraft server",
  "authors": [
    "AlexDevs"
  ],
  "contact": {
    "sources": "https://github.com/ReconnectedCC/rcc-chatbox"
  },
  "license": "MIT License",
  "icon": "assets/ct-chatbox/icon.png",
  "environment": "server",
  "entrypoints": {
